{
  "name": "@arethetypeswrong/history",
  "version": "0.3.0",
  "type": "module",
  "files": [
    "src",
    "lib",
    "./data/full.json.gz",
    "./data/dates.json"
  ],
  "exports": {
    ".": {
      "types": "./lib/main.d.ts",
      "default": "./src/main.js"
    },
    "./utils": {
      "types": "./lib/utils.d.ts",
      "default": "./src/utils.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "build:scripts": "tsc -b scripts",
    "pregenerate": "npm run build:scripts",
    "generate": "tsx scripts/generateFull.ts",
    "prepublishOnly": "npm run build && npm run generate"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "file:../core",
    "@azure/storage-blob": "^12.14.0",
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.4",
    "@types/pacote": "^11.1.5",
    "@types/semver": "^7.5.0",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "npm-high-impact": "^1.3.0",
    "pacote": "^15.2.0",
    "semver": "^7.5.3",
    "ts-expose-internals": "^5.2.2",
    "tsx": "^3.12.7",
    "types-registry": "latest",
    "typescript": "^5.2.2"
  }
}
